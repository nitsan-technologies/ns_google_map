{
  "name": "nitsan/ns-google-map",
  "type": "typo3-cms-extension",
  "description": "Introducing the TYPO3 Google Map Extension! This TYPO3 extension enables you to seamlessly add and customize Google Maps to your website with ease. Designed with responsive layouts and SEO-friendly features, it ensures a smooth and optimized experience for your visitors.",
  "homepage": "https://t3planet.com/typo3-google-map-extension",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "Google",
    "Map",
    "Search",
    "Direction",
    "NITSAN"
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_google_map/issues"
  },
  "require": {
    "typo3/cms-core": "^12"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsGoogleMap\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/ns-google-map": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_google_map"
    }
  }
}
